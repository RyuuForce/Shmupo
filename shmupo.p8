 print("congratulations!",35,50,7)
 print("you saved our planet!",25,62,7)
 print("press üÖæÔ∏è to retry!",31,74,blink_y)
end

function drawstars()
 for i = 1, #stars do
  circfill(stars[i].x,stars[i].y,stars[i].z,stars[i].c)  
 end
end
